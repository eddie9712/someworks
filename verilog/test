#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xc60730 .scope module, "t_simple_c" "t_simple_c" 2 13;
 .timescale -9 -10;
v0xca5660_0 .var "A", 0 0;
v0xca5700_0 .var "B", 0 0;
v0xca57b0_0 .var "C", 0 0;
v0xca5860_0 .net "D", 0 0, L_0xca5ca0; 1 drivers
v0xca5940_0 .net "E", 0 0, L_0xca5b60; 1 drivers
S_0xc60820 .scope module, "test" "simple_c" 2 17, 2 3, S_0xc60730;
 .timescale -9 -10;
L_0xca59f0/d .functor AND 1, v0xca5660_0, v0xca5700_0, C4<1>, C4<1>;
L_0xca59f0 .delay (300,300,300) L_0xca59f0/d;
L_0xca5b60/d .functor NOT 1, v0xca57b0_0, C4<0>, C4<0>, C4<0>;
L_0xca5b60 .delay (100,100,100) L_0xca5b60/d;
L_0xca5ca0/d .functor OR 1, L_0xca59f0, L_0xca5b60, C4<0>, C4<0>;
L_0xca5ca0 .delay (200,200,200) L_0xca5ca0/d;
v0xc960e0_0 .net "A", 0 0, v0xca5660_0; 1 drivers
v0xca52f0_0 .net "B", 0 0, v0xca5700_0; 1 drivers
v0xca5390_0 .net "C", 0 0, v0xca57b0_0; 1 drivers
v0xca5430_0 .alias "D", 0 0, v0xca5860_0;
v0xca54e0_0 .alias "E", 0 0, v0xca5940_0;
v0xca5580_0 .net "w1", 0 0, L_0xca59f0; 1 drivers
    .scope S_0xc60730;
T_0 ;
    %vpi_call 2 21 "$dumpfile", "test.vcd";
    %vpi_call 2 22 "$dumpvars";
    %end;
    .thread T_0;
    .scope S_0xc60730;
T_1 ;
    %set/v v0xca5660_0, 0, 1;
    %set/v v0xca5700_0, 0, 1;
    %set/v v0xca57b0_0, 0, 1;
    %delay 1000, 0;
    %set/v v0xca5660_0, 1, 1;
    %set/v v0xca5700_0, 1, 1;
    %set/v v0xca57b0_0, 1, 1;
    %end;
    .thread T_1;
    .scope S_0xc60730;
T_2 ;
    %delay 2000, 0;
    %vpi_call 2 30 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "test.v";
